{
  "dns": {
    "hosts": {
      "miwifi.com": ["10.0.0.1"],
      "doh.pub": ["1.12.12.12", "120.53.53.53", "2402:4e00::"],
      "dns.alidns.com": [
        "223.5.5.5",
        "223.6.6.6",
        "2400:3200::1",
        "2400:3200:baba::1"
      ],
      "dns.google": [
        "8.8.8.8",
        "8.8.4.4",
        "2001:4860:4860::8888",
        "2001:4860:4860::8844"
      ],
      "cloudflare-dns.com": [
        "1.1.1.1",
        "1.0.0.1",
        "2606:4700:4700::1111",
        "2606:4700:4700::1001"
      ]
    },
    "servers": [
      { "tag": "dns_block", "address": "rcode://success" },
      {
        "tag": "dns_direct",
        "address": "https://dns.alidns.com/dns-query",
        "strategy": "prefer_ipv6",
        "detour": "DIRECT"
      },
      {
        "tag": "dns_proxy",
        "address": "https://cloudflare-dns.com/dns-query",
        "strategy": "prefer_ipv4"
      },
      {
        "tag": "local",
        "address": "local",
        "detour": "DIRECT"
      },
      { "tag": "dns_fakeip", "address": "fakeip" }
    ],
    "rules": [
      { "outbound": "any", "server": "dns_direct" },
      { "clash_mode": "Global", "server": "dns_fakeip" },
      { "clash_mode": "Direct", "server": "dns_direct" },
      {
        "rule_set": ["ads", "adb-1", "adb-2", "ads-p", "privacy"],
        "server": "dns_block"
      },
      {
        "rule_set": [
          "google-fcm",
          "google",
          "telegram",
          "microsoft-bing",
          "microsoft-cloud",
          "microsoft-service",
          "openai",
          "gemini",
          "conjbai",
          "server",
          "youtube",
          "netflix",
          "baha",
          "media-proxy",
          "proxy"
        ],
        "query_type": ["A", "AAAA"],
        "rewrite_ttl": 1,
        "disable_cache": true,
        "server": "dns_fakeip"
      },
      {
        "rule_set": ["games-1", "games-2", "games-3", "games-4", "games-5"],
        "server": "dns_proxy"
      },
      {
        "rule_set": [
          "direct-0",
          "direct-1",
          "google-cn",
          "direct-3",
          "apple",
          "netease-music",
          "bilibili",
          "media-cn",
          "cn",
          "cn-ip",
          "cloud-cn",
          "download"
        ],
        "server": "dns_direct"
      },
      {
        "query_type": ["A", "AAAA"],
        "rewrite_ttl": 1,
        "disable_cache": true,
        "server": "dns_fakeip"
      }
    ],
    "final": "dns_proxy",
    "independent_cache": true,
    "reverse_mapping": true,
    "fakeip": {
      "enabled": true,
      "inet6_range": "fc00::/15",
      "inet4_range": "198.18.0.0/15"
    }
  }
}
